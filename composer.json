{
	"name": "user-role-adjustments/user-role-adjustments",
	"description": "A WordPress plugin for User Role Adjustments",
	"type": "project",
	"authors": [
		{
			"name": "Designzillas",
			"email": "info@designzillas.com"
		}
	],
	"require": {},
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.2",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
		"phpcompatibility/php-compatibility": "^9.3",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": { "User_Role_Adjustments\\": "./src" }
	},
	"scripts": {
		"install-codestandards": [
			"Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
		],
		"phpcs": "phpcs . --ignore=node_modules/*,vendor/*"
	}
}
